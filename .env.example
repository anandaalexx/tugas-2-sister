# example .env

# Node Configuration
NODE_ID=node-1
PORT=8001
REGION=US
LOG_LEVEL=INFO

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# Cluster Configuration
PEERS=node-2:http://node2:8002,node-3:http://node3:8003

# Cache Configuration
CACHE_CAPACITY=100

# Raft Configuration
HEARTBEAT_INTERVAL=50
ELECTION_TIMEOUT_MIN=150
ELECTION_TIMEOUT_MAX=300

# Deadlock Detection
DEADLOCK_CHECK_INTERVAL=5

# Queue Configuration
REPLICATION_FACTOR=3
WRITE_QUORUM=2

# Performance Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090
